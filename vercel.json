{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install -g playwright && npm install && npx playwright install chromium --with-deps"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "/tmp"
  }
} 